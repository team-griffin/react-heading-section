{
  "name": "@team-griffin/react-heading-section",
  "version": "2.3.0",
  "main": "dist/cjs/react-heading-section.js",
  "module": "dist/es/react-heading-section.js",
  "repository": "git@github.com:team-griffin/react-heading-section.git",
  "author": "Joshua Thornton <joshua.thornton@heg.com>",
  "license": "MIT",
  "scripts": {
    "prepublish": "npm run build",
    "transpile": "babel src -d lib --copy-files",
    "lint": "npm run lint:js && npm run flow",
    "lint:js": "eslint -c ./config/.eslintrc.js 'src/**/*.js'",
    "lint:js:fix": "eslint --fix -c ./config/.eslintrc.js 'src/**/*.js'",
    "flow": "flow check; test $? -eq 0 -o $? -eq 2",
    "build:dev": "cross-env NODE_ENV=development rollup --config ./rollup/development.js",
    "build:prod": "cross-env NODE_ENV=production rollup --config ./rollup/production.js",
    "build:entry": "cross-env NODE_ENV=production rollup --config ./rollup/entry.js",
    "build": "yarn build:dev && yarn build:prod && yarn build:entry",
    "ci": "npm run lint && npm run test && npm run build",
    "postmerge": "yarn",
    "storybook": "start-storybook -h 0.0.0.0 -p 9001 -c .storybook",
    "test": "mocha --require babel-polyfill --compilers js:babel-register 'src/**/__tests__/*' -R spec",
    "dockerfile:ci": "dockerscript ./docker/ci/dockerfile.js ./docker/ci/Dockerfile"
  },
  "devDependencies": {
    "@storybook/react": "^3.2.12",
    "@team-griffin/eslint-config": "^3.0.3",
    "@team-griffin/install-self-peers": "^1.0.0",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.1.21",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-object-rest-spread": "^0.0.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.5.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "chai": "^4.1.2",
    "cross-env": "^5.1.3",
    "dockerscript": "^0.0.6",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "eslint": "^4.13.0",
    "eslint-plugin-flowtype": "^2.30.4",
    "eslint-plugin-react": "^7.5.1",
    "mocha": "^4.0.1",
    "react": "^16.0.0",
    "react-test-renderer": "^16.0.0",
    "rollup": "^0.55.3",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-local-resolve": "^1.0.7",
    "rollup-plugin-memory": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify-es": "^0.0.1"
  },
  "dependencies": {
    "prop-types": "^15.6.0",
    "react-broadcast": "^0.6.0",
    "recompose": "^0.26.0"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  }
}
